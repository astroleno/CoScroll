# Tripo3D API 配置示例
# 请复制此文件为 .env.local 并填入您的真实API密钥

# Tripo3D API密钥 (必需)
FAL_KEY=YOUR_API_KEY_HERE

# 可选配置
# API基础URL (通常不需要修改)
TRIPO3D_BASE_URL=https://api.tripo3d.ai/v1

# 模型生成参数
MODEL_QUALITY=high
MODEL_FORMAT=glb
MODEL_RESOLUTION=1024
MODEL_DETAIL_LEVEL=8

# 批量处理设置
BATCH_SIZE=5
MAX_RETRIES=3
RETRY_DELAY=2

# 日志设置
LOG_LEVEL=INFO
LOG_FILE=./png2model.log
