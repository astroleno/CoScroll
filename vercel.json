{
  "headers": [
    {
      "source": "/(.*)\\.(mp3|obj|hdr|ttf|otf|woff|woff2|png|jpg|jpeg|webp)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 30
    }
  }
}
